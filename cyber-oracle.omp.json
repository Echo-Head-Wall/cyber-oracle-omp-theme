{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }} - Oracle's Navigation Terminal üß°",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#FF1493",
          "template": "{{ if gt .Code 0 }}üíî Error {{ .Code }} - Hey, listen! Something went wrong!{{ end }}"
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#FF0000",
          "template": "{{ if .Root }}‚ö†Ô∏è ROOT ACCESS - Be careful! ‚ö†Ô∏è{{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FF0000",
          "background": "#1a1a1a",
          "template": "{{ if .Env.PRODUCTION_SERVER }}üö® PRODUCTION üö®{{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFA500",
          "template": "{{ if .Env.STAGING_SERVER }}üöß STAGING{{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#32CD32",
          "template": "{{ if .Env.DEV_SERVER }}üß™ DEV{{ end }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#9A4AE2",
          "properties": {
            "display_version": true,
            "fetch_version": true,
            "display_mode": "context"
          },
          "template": "üêç{{ .Full }} "
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#68D391",
          "properties": {
            "display_version": false,
            "fetch_version": true,
            "display_mode": "context"
          },
          "template": "‚¨¢{{ .Major }}.{{ .Minor }} "
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "#E94E2D",
          "properties": {
            "display_version": true,
            "fetch_version": true,
            "display_mode": "context"
          },
          "template": "ü¶Ä{{ .Full }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#00ADD8",
          "properties": {
            "display_version": true,
            "fetch_version": true,
            "display_mode": "context"
          },
          "template": "üêπ{{ .Full }} "
        },
        {
          "type": "docker",
          "style": "plain",
          "foreground": "#2496ED",
          "properties": {
            "display_mode": "context"
          },
          "template": "üê≥{{ .Context }} "
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "#326CE5",
          "properties": {
            "parse_kubeconfig": true
          },
          "template": "‚öôÔ∏è{{ .Context }}{{ if .Namespace }}:{{ .Namespace }}{{ end }} "
        },
        {
          "type": "aws",
          "style": "plain",
          "foreground": "#FFA500",
          "template": "‚òÅÔ∏è{{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} "
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#FFFF00",
          "properties": {
            "charged_icon": "üîã",
            "charging_icon": "üîå", 
            "discharging_icon": "üîã"
          },
          "template": " {{ .Icon }}{{ .Percentage }}% "
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#FF6B35",
          "properties": {
            "threshold": 500,
            "style": "round"
          },
          "template": "{{ if gt .Ms 500 }}‚è±Ô∏è{{ .FormattedMs }}{{ end }}"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#39FF14",
          "properties": {
            "style": "agnoster_short"
          },
          "template": "üìÇ {{ .Path }} "
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#FF6B35",
          "properties": {
            "branch_icon": "üåø",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "{{ if .HEAD }}<#39FF14>git:(</>{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .UpstreamIcon }} {{ .UpstreamIcon }}{{ end }}<#39FF14>)</>{{ if .Working.Changed }} <#FF1493>‚óè{{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <#FFFF00>+{{ .Staging.String }}</>{{ end }}{{ if .Untracked }} <#E94E2D>?{{ .Untracked }}</>{{ end }}{{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#00FFFF",
          "template": "{{ if .Env.SSH_CONNECTION }}üîó{{ end }}üñ•Ô∏è{{ .HostName }} {{ if .Env.ENVIRONMENT }}[{{ .Env.ENVIRONMENT }}]{{ end }}{{ if .Env.TMUX }}üì∫tmux{{ end }}{{ if .Env.STY }}üì∫screen{{ end }} <#FFB6C1>[{{ .Shell }}]</> <#00FFFF>{{ .CurrentDate | date \"15:04\" }}</> <#FF1493>üíä</>"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "<#39FF14>oracle</><#FF6B35>@</><#00FFFF>{{ .HostName }}</><#FF6B35>:</><#FFFF00>{{ .PWD }}</><#FF6B35>$ </>"
        }
      ]
    }
  ],
  "secondary_prompt": {
    "foreground": "#FF6B35",
    "template": "<#39FF14>Oracle:</> <#FF6B35>Compiling thoughts... Need more coffee? ‚òï</> "
  },
  "transient_prompt": {
    "foreground": "#39FF14", 
    "template": "<#39FF14>></> "
  }
}
